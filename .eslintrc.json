{
  "env": {
    "browser": true,
    "es2021": true,
    "webextensions": true,
    "jest": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  },
  "globals": {
    "chrome": "readonly",
    "global": "writable"
  }
}
